<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Subject" table="subject">
        <id name="id" column="subject_id">
            <generator class="assigned"/>
        </id>
        <property name="name" column="subject_name"/>
        <property name="description" column="subject_description"/>
        <property name="level" column="subject_level"/>
    </class>
</hibernate-mapping>
